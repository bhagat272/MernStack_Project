{
    "version": 2,
    "builds": [
      {
        "src": "server.js",  // Replace with the path to your main server file
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"  // Ensure this matches the entry point of your backend
      }
    ]
  }
  